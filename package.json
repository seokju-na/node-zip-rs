{
  "name": "node-zip-rs",
  "version": "0.0.6",
  "description": "Zip library for Node.js with Rust binding",
  "main": "index.js",
  "types": "index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/seokju-na/node-zip-rs.git"
  },
  "keywords": [
    "napi-rs",
    "zip",
    "archive",
    "unzip"
  ],
  "homepage": "https://github.com/seokju-na/node-zip-rs",
  "author": "Seokju Na <seokju.me@gmail.com>",
  "bugs": {
    "url": "https://github.com/seokju-na/node-zip-rs/issues"
  },
  "napi": {
    "name": "node-zip-rs",
    "triples": {
      "defaults": false,
      "additional": [
        "aarch64-apple-darwin",
        "aarch64-pc-windows-msvc",
        "x86_64-apple-darwin",
        "x86_64-pc-windows-msvc",
        "i686-pc-windows-msvc",
        "universal-apple-darwin",
        "x86_64-unknown-linux-gnu",
        "x86_64-unknown-linux-musl"
      ]
    }
  },
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@napi-rs/cli": "^2.18.4",
    "@types/node": "^22.10.1",
    "@types/yauzl-promise": "^4",
    "vitest": "^2.1.8",
    "yauzl-promise": "^4.0.0"
  },
  "files": [
    "index.js",
    "index.d.ts"
  ],
  "engines": {
    "node": ">= 10"
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --platform --release",
    "build:debug": "napi build --platform",
    "prepublishOnly": "napi prepublish -t npm",
    "test": "vitest run",
    "bench": "vitest bench --run",
    "check": "biome check",
    "check:fix": "biome check --write --unsafe",
    "version": "napi version"
  },
  "packageManager": "yarn@4.5.3"
}
